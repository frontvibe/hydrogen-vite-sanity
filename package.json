{
  "name": "hydrogen-vite-sanity",
  "private": true,
  "sideEffects": false,
  "version": "2024.7.10",
  "type": "module",
  "scripts": {
    "build": "npm run sanity:typegen && shopify hydrogen build --codegen",
    "dev": "npm run sanity:typegen && shopify hydrogen dev --codegen",
    "preview": "shopify hydrogen preview --build",
    "lint": "eslint --no-error-on-unmatched-pattern --ext .js,.ts,.jsx,.tsx .",
    "typecheck": "npm run codegen && npm run sanity:typegen && tsc --noEmit",
    "codegen": "shopify hydrogen codegen",
    "sanity:typegen": "npm run sanity:extract && npm run sanity:generate",
    "sanity:extract": "sanity schema extract --path ./types/sanity/schema.json",
    "sanity:generate": "sanity typegen generate"
  },
  "prettier": "@shopify/prettier-config",
  "dependencies": {
    "@remix-run/react": "^2.13.1",
    "@remix-run/server-runtime": "^2.13.1",
    "@sanity/client": "^6.22.2",
    "@sanity/preview-url-secret": "^2.0.0",
    "@sanity/react-loader": "^1.10.14",
    "@sanity/vision": "^3.62.2",
    "@sanity/visual-editing": "^2.4.2",
    "@shopify/hydrogen": "2024.10.0",
    "@shopify/remix-oxygen": "^2.0.9",
    "graphql": "^16.9.0",
    "graphql-tag": "^2.12.6",
    "groq": "^3.62.2",
    "isbot": "^3.8.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sanity": "^3.62.2",
    "tailwindcss": "4.0.0-alpha.30"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "5.0.2",
    "@remix-run/dev": "^2.13.1",
    "@remix-run/eslint-config": "^2.13.1",
    "@shopify/cli": "~3.68.1",
    "@shopify/hydrogen-codegen": "^0.3.2",
    "@shopify/mini-oxygen": "^3.1.0",
    "@shopify/oxygen-workers-types": "^4.1.6",
    "@shopify/prettier-config": "^1.1.2",
    "@tailwindcss/vite": "4.0.0-alpha.17",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/eslint": "^8.56.12",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.1",
    "eslint-plugin-hydrogen": "0.12.2",
    "graphql-config": "^5.1.3",
    "prettier": "^2.8.8",
    "typescript": "^5.6.3",
    "vite": "^5.4.10",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "browserslist": [
    "defaults"
  ]
}
